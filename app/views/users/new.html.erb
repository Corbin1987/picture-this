<div class="form-spacing">

  <div class="form-group">
    <%= bootstrap_form_for(@user) do |f| %>
      <%= f.text_field :first_name %>
      <%= f.text_area :last_name %>
      <%= f.text_area :username %>
      <%= f.text_area :email %>
      <%= f.password_field :password %>
      <%= f.submit %>
    <% end %>

</div>
